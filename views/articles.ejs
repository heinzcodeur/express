<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Liste des Articles</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1>Liste des Articles</h1>
    <ul>
      <% articles.forEach(function(article) { %>
        <li>
          <h2><%= article.title %></h2>
          <p><%= article.content %></p>
        </li>
      <% }); %>
    </ul>
</body>
</html>
